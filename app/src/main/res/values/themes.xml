<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ViewModelTest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/material_dynamic_primary40</item>
        <item name="colorPrimaryVariant">@color/material_dynamic_neutral70</item>
        <item name="colorOnPrimary">@color/material_dynamic_primary95</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/material_dynamic_secondary80</item>
        <item name="colorSecondaryVariant">@color/material_dynamic_secondary40</item>
        <item name="colorOnSecondary">@color/material_dynamic_secondary20</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:colorBackground">@color/material_dynamic_neutral95</item>
        <item name="android:textColor">@color/material_dynamic_primary30</item>
    </style>
</resources>